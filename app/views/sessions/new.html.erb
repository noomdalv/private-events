<% provide(:title, 'Log In') %>
<div class="row session">
  <h1 class="text-center">Log In</h1>
  <div class="col-md-12">
    <%= form_for(:session) do |f| %>
      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control' %><br>

      <%= f.submit "Log In", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
